
<body>
  <h1>
    <%= title %>
  </h1>
  <p>
    Current second:
      <%= seconds %>
  </p>
  <% if (seconds % 2===0) { %>
  <ul>
    <% for(let i = 0 ; i<items.length; i++) { %>
    <li>
      <%= items[i]  %>
    </li>
    <% } %>
  </ul>
 <% } else { %>
  <p>No items to display</p>
  <% } %>
  <p>
  <%- htmlContent%>
  </p>

  <%- include("footer.ejs")  %>
</body>